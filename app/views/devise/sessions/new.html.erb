

<div class="col-lg-1" style="background-color: white;">
        </div>

      <div class="col-lg-11">
        <h2>Log in</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div><%= f.label :name %><br/>
  <%= f.text_field :name, autofocus: true, class: "name" %></div>

  <div><%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off", class: "password" %></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div><%= f.submit "Log in" ,class: 'btn btn-primary'%></div>
<% end %>

<%= render "devise/shared/links", class: "sign_up" %>


        
      </div>


  
